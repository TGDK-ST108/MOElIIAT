# ====================================================================

[iris_scan]
initiated=TRUE
camera_mode=infrared
capture_window=720ms
entropy_map=active
scan_points=468
retina_gate=enabled
ocular_vector_signature=bound_to_SEAN_TICHENOR

[verification]
hash_mode=SHA-512-QQUAp
linked_retainer=TGDK-CAP-01
match_threshold=0.992
biometric_seal=TRUE
visual_auth_required=TRUE
audio_auth_phrase="Invoke OliviaAI, Retain Trust"

[restrictions]
geo_lock=ENFORCED
authorized_region=US
seal=Y_SEAL_ACTIVE
foreign_key_bypass=DENIED
offline_use=DISALLOWED

[logistics]
output_vector=retainer_iris_lock.qfx
match_result=APPEND
license_enforced=D2501-V01
